{
  "name": "@keagan-ladds/stargazer-ui",
  "version": "2.1.9",
  "description": "UI Kit built on top of Bootstrap 4, based on stargazer.",
  "keywords": [
    "bootstrap",
    "css",
    "dashboard",
    "framework",
    "front-end",
    "responsive",
    "sass",
    "ui kit",
    "webapp"
  ],
  "homepage": "https://github.com/keagan-ladds/stargazer-ui",
  "license": "MIT",
  "author": {
    "name": "Keagan Ladds",
    "github": "https://github.com/keagan-ladds"
  },
  "files": [
    "dist/",
    "js/",
    "scss/"
  ],
  "main": "dist/js/stargazer.js",
  "sass": "scss/stargazer.scss",
  "style": "dist/css/stargazer.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/keagan-ladds/stargazer-ui.git"
  },
  "scripts": {
    "copy-bootstrap": "rimraf scss/bootstrap && copyfiles -a \"node_modules/bootstrap/scss/**/*\" -u 3 scss/bootstrap",
    "css": "npm-run-all --parallel css-lint css-compile* --sequential css-prefix css-minify*",
    "css2": "npm-run-all --parallel css-compile* --sequential css-prefix css-minify*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/stargazer.scss dist/css/stargazer.css",
    "css-lint": "stylelint --config build/.stylelintrc --syntax scss \"scss/**/*.scss\"",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/stargazer.min.css dist/css/stargazer.css",
    "css-compile-bootstrap": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/bootstrap.scss dist/css/bootstrap.css",
    "css-minify-bootstrap": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/bootstrap.min.css dist/css/bootstrap.css",
    "dist": "npm-run-all copy-bootstrap --parallel css js",
    "increment-version": "node build/increment-version.js",
    "js": "npm-run-all js-lint js-compile* js-minify*",
    "js-lint": "eslint js/",
    "js-compile": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "cross-env PLUGINS=true babel js/src/ --out-dir js/dist/ --source-maps",
    "js-compile-utilities": "rollup --environment BUNDLE:false --config build/rollup.config.utilities.js --sourcemap",
    "js-minify": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/stargazer.js.map,includeSources,url=stargazer.min.js.map\" --output dist/js/stargazer.min.js dist/js/stargazer.js",
    "js-minify-utilities": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/stargazer-utilities.js.map,includeSources,url=stargazer-utilities.min.js.map\" --output dist/js/stargazer-utilities.min.js dist/js/stargazer-utilities.js",
    "postinstall": "stargazer-plugin-npm-postinstall || exit 0",
    "release-version": "node build/change-version.js",
    "release-zip": "cd dist/ && zip -r9 stargazer-$npm_package_version-dist.zip * && shx mv stargazer-$npm_package_version-dist.zip ..",
    "watch": "npm-run-all --parallel watch-css watch-js",
    "watch-js": "nodemon --ignore js/dist/ --ignore dist/ -e js -x \"npm-run-all js-compile* js-minify*\"",
    "watch-css": "nodemon --ignore dist/ -e scss -x \"npm run css\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "clean-css-cli": "^4.2.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-plugin-compat": "^2.7.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semver": "^5.6.0",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "uglify-js": "^3.5.1"
  },
  "peerDependencies": {
    "jquery": "1.9.1 - 3",
    "perfect-scrollbar": "^1.3.0",
    "popper.js": "^1.14.3"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
